{
    "Initializer BoilerPlate":{
    "prefix": "init",
    "body": [
        "//=====================COMPILER OPTIMIZATION STARTS=======================",
        "#pragma GCC optimize(\"Ofast\")",
        "#pragma GCC target(\"avx,avx2,fma\")",
        "#pragma GCC optimization (\"O3\")",
        "#pragma GCC optimization (\"unroll-loops\")",
        "//======================COMPILER OPTIMIZATION ENDS=======================",
        "",
        "//compile using : g++ -D DEB file.cpp",
        "#ifdef DEB",
        "\t//to check from out of bounds errors",
        "\t#define _GLIBCXX_DEBUG",
        "#endif",
        "//=============================DEBUG PART ENDS===========================",
        "",
        "#include<bits/stdc++.h>",
        "using namespace std;",
        "",
        "#define ll long long int",
        "#define pi pair<ll,ll>",
        "",
        "ll power(ll x, ll y, ll mod = 1e9+7){",
        "\tll res = 1;",
        "\twhile(y > 0){",
        "\t\tif(y & 1)",
        "\t\t\tres = (res * x) % mod;",
        "\t\ty = y >> 1;",
        "\t\tx = (x*x)% mod;",
        "\t}",
        "\treturn res % mod;",
        "}",
        "",
        "void solve(){",
        "\t$0\n",
        "}",
        "",
        "int main(){",
        "\tios_base::sync_with_stdio(false);",
        "\tcin.tie(0);\tcout.tie(0);",
        "",
        "\tint test = 1;",
        "\tcin >> test;$1",
        "\twhile(test--)",
        "\t\tsolve();",
        "\treturn 0;",
        "}"

    ],
    "description": "Initializer boilerplate template"
}

